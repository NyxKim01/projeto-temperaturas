import pandas as pd

df = pd.read_csv("INMET/dados/2025.CSV", skiprows=8, encoding="latin1", sep=";")
df.columns = [
    "Data",
    "Hora",
    "1",
    "2",
    "3",
    "4",
    "5",
    "Temperatura",
    "Orvalho",
    "Max",
    "Min",
    "O_max",
    "O_min",
    "U_max",
    "U_min",
    "Umidade",
    "6",
    "7",
    "8",
    "9",
    ]
df = df.drop(columns=["1", "2", "3", "4", "5", "6", "7", "8", "9"])
df = df.dropna()
df = df.reset_index(drop=True)
print(df)